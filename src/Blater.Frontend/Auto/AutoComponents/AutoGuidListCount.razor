@inherits Blater.Frontend.Auto.Bases.BaseValueAutoComponent<List<Guid>>
@implements IAutoBuildableComponent

<div id="auto--guid-list-count" class="@ExtraClass">
    @if (!string.IsNullOrEmpty(LabelName))
    {
        <MudText>@LabelName</MudText>
    }
    <MudText>@Value.Count</MudText>

</div>


@code {
#pragma warning disable CA2252
    public int Priority => 1000;

    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoGuidListCount;
#pragma warning restore CA2252
}