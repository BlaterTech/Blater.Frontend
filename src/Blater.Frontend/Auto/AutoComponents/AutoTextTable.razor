@typeparam TItemType where TItemType : IConvertible
@inherits Blater.FrontEnd.Auto.Bases.BaseValueAutoComponent<TItemType>
@using Blater.Enumerations.AutoModel
@implements IAutoBuildableComponent

<div id="auto-text-table" class="@ExtraClass">
    @if (!string.IsNullOrEmpty(LabelName))
    {
        <MudText>@LabelName</MudText>
    }
    <MudText>@Value</MudText>
</div>

@code {
    public int Priority => 1000;

    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoTextTable;
}