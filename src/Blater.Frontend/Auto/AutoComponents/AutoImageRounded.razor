@inherits Blater.Frontend.Auto.Bases.BaseValueAutoImageComponent
@implements IAutoBuildableComponent

<div id="auto-image-rounded" class="@ExtraClass">
    @if (!string.IsNullOrEmpty(LabelName))
    {
        <MudText>@LabelName</MudText>
    }
    <MudAvatar>
        <MudImage
            Src="@Value"
            Elevation="25"
            Class="rounded-lg"
            Style="@ImageStyle"/>
    </MudAvatar>
</div>


@code {
#pragma warning disable CA2252
    public int Priority => 1000;

    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoImageRounded;
#pragma warning restore CA2252
}