@inherits Blater.FrontEnd.Auto.Bases.BaseValueAutoComponent<Guid>
@using Blater.Enumerations.AutoModel
@implements Blater.FrontEnd.Auto.IAutoBuildableComponent

<div id="auto-short-id" class="@ExtraClass">
    @if (!string.IsNullOrEmpty(LabelName))
    {
        <MudText>@LabelName</MudText>
    }
    <MudText>
        @Value.ToString().Split('-').First()
    </MudText>
</div>


@code {

    public int Priority => 1000;

    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoShortId;
}