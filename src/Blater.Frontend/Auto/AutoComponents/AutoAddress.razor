@*@inherits Blater.Frontend.Auto.Bases.BaseValueAutoComponent<Address>
@using Blater.Frontend.Enumerations.AutoModel
@implements Blater.Frontend.Auto.IAutoBuildableComponent

<div id="auto-address" class="@ExtraClass">
    @if (!string.IsNullOrEmpty(LabelName))
    {
        <MudText>@LabelName</MudText>
    }
    <MudText>@Address</MudText>
</div>

@code {

    public int Priority => 1000;

    private string Address => $"{Value.Street} {Value.Number}, {Value.District} - {Value.PostalCode} - {Value.City}, {Value.State}";

    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoAddress;


}*@