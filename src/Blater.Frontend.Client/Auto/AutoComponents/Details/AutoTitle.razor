@implements IAutoBuildableComponent
@inherits BaseAutoComponent

<div id="auto-text" class="@ExtraClass">
    <MudText Typo="Typo.h6">@Value</MudText>
</div>


@code {
    public BaseAutoComponentTypeEnumeration ComponentType => AutoComponentType.AutoTitle;

    [Parameter]
    public string? Value { get; set; }
}